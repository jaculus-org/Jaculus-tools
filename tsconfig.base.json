{
  "compilerOptions": {
    "target": "ES2021",
    "module": "ES2020",
    "moduleResolution": "Node",
    "baseUrl": ".",
    "paths": {
      "@jaculus/util/*": ["packages/util/src/*"],
      "@jaculus/link/*": ["packages/link/src/*"],
      "@jaculus/package/*": ["packages/package/src/*"],
      "@jaculus/code/*": ["packages/code/src/*"],
      "@jaculus/commands/*": ["packages/commands/src/*"],
      "@jaculus/device/*": ["packages/device/src/*"]
    },
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "outDir": "dist",
    "rootDir": "src",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "strict": true,
    "skipLibCheck": true
  }
}
